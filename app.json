{
  "expo": {
    "name": "Krio Audio Bible",
    "slug": "krio-audio-bible",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0e27"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.newlifeinkossohtown.krioaudiobible"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0a0e27"
      },
      "package": "com.newlifeinkossohtown.krioaudiobible",
      "versionCode": 2,
      "permissions": [
        "INTERNET"
      ]
    },
    "plugins": [
      "expo-av"
    ],
    "extra": {
      "eas": {
        "projectId": "32224755-5543-4a81-92b7-8984f06d30d9"
      }
    }
  }
}
